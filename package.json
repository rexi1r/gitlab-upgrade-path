  "scripts": {
    "postinstall": "node scripts/patch-babel-polyfill.js",
    "start": "cross-env GENERATE_SOURCEMAP=false BROWSER=none DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "generate": "npm run build",
    "build:local": "cross-env DISABLE_ESLINT_PLUGIN=true PUBLIC_URL=/ react-scripts build",
    "build-localhost": "cross-env DISABLE_ESLINT_PLUGIN=true PUBLIC_URL=/ react-scripts build",
    "serve": "serve -s build -l 8000",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.28.6",
    "cross-env": "^10.1.0",
    "serve": "^14.2.5"
