    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^9.0.1",
